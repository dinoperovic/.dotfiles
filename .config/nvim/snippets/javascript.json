{
  "log": {
    "prefix": "log",
    "body": ["console.log({$0})"],
    "description": "log a variable to the console"
  },
  "log-stringify": {
    "prefix": "logstr",
    "body": ["console.log('$1', JSON.stringify($0, null, 2))"],
    "description": "log a stringified variable to the console"
  },
  "stringify": {
    "prefix": "str",
    "body": ["JSON.stringify($0)"],
    "description": "stringify a variable"
  },
  "script": {
    "prefix": "scr",
    "body": ["<script lang=\"ts\">", "\t$0", "</script>"],
    "description": "script ts tag"
  }
}
